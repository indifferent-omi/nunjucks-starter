{% macro blog(obj) %}
    <div class="card-blog">
        {% if obj.image %}
            <div class="card-image">
                <img src="{{obj.image}}" alt="">
            </div>
        {% endif %}
        <div class="card-inner">
            <div class="blog-meta">
            <a href="">Admin</a>
            <a href="">17 November, 2021</a>
            </div>
            <h3 class="card-title">How to Create a Referral Program in WordPress backup plugins</h3>
            <p>It is almost impossible to read  coming across a lead story elections through fake social media accounts...</p>
            <a href="" class="btn btn-blue-ribbon--outline rounded-full border-2">Read More</a>
        </div>
    </div>
{% endmacro %}

{% macro commentCard(obj) %}
    <div class="card-user">
        <img src="{{obj.img}}" alt="user">
    </div>
    <div class="card-content">
        <p class="text-dark">{{obj.text}}</p>
        <div class="card-content__info">
            <div class="user-detail">
                <h6>{{obj.name}}</h6>
                <span class="text-dark">{{obj.date}}</span>
            </div>
            <div class="user-respons">
                <button class="text-dark">Reply</button>
            </div>
        </div>
    </div>
{% endmacro %}


{% macro portfolio(obj) %}
    <div class="card card-portfolio{{" "+ obj.modifier if obj.modifier}}">
          <img src="{{obj.image}}" alt="portfolio" class="w-100">
        <div class="card-texts">
            <h3 class="card-title">{{obj.title}}</h3>
            <span>{{obj.text}}</span>
            <a href="#" class="btn">Full View</a>
        </div>
    </div>
{% endmacro %}

{% macro career(obj) %}
    <div class="card card-career">
        <h3 class="card-title">{{obj.title}}</h3>
        <p>{{obj.text}}</p>
        <ul class="card-list list-unstyled">
            <li><span class="icon"><img src="./image/icons/icon-map-icon.svg" alt="icon"></span><span>{{obj.location}}</span></li>
            <li><span class="icon"><img src="./image/icons/icon-clock.svg" alt="icon"></span><span>{{obj.jobType}}</span></li>
        </ul>
        <a href="" class="card-button">Learn the details <i class="fas fa-arrow-right"></i></a>
    </div>
{% endmacro %}