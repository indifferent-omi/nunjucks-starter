{% macro One(obj) %}
  <div class=" card-pricing--01 text-center {{obj.modifier}}">
    <div class="card-pricing--01__head">
      <h3 class="card-pricing--01__plan-title{{" " + obj.planTextColor if obj.planTextColor}}">{{obj.plan}} Plan</h3>
      <h2 class="card-pricing--01__price">{{obj.price}}</h2>
      <span class="card-pricing--01__plan-text">Per Month</span>
    </div>
    <div class="d-flex justify-content-center">
      <ul class="card-pricing--01__list list-unstyled  text-start">
          {% for item in obj.list %}
            <li class="d-flex">
              <i class="fas fa-check"></i>
              <span>{{item.text}}</span>
            </li>  
          {% endfor %}
      </ul>
    </div>
    <div class="card-pricing--01__button text-center">
      <a href="#" class="btn btn-60 {{" "+ obj.btnClass if obj.btnClass}}{{" active" if obj.active}}">Get Started</a>
    </div>
  </div>
{% endmacro %}
{% macro Two(obj) %}
   <div class=" card-pricing--01 {{obj.modifier}}">
    <div class="card-pricing--01__head text-center">
      <h3 class="card-pricing--01__plan-title{{" " + obj.planTextColor if obj.planTextColor}}">{{obj.plan}} Plan</h3>
      <h2 class="card-pricing--01__price dynamic-value" data-yearly="{{obj.price.yearly}}" data-monthly="{{obj.price.monthly}}" data-active="{{obj.price.yearly}}"></h2>
      <span class="card-pricing--01__plan-text">Per Month</span>
    </div>
    <div class="d-flex justify-content-center">
      <ul class="card-pricing--01__list list-unstyled  text-center">
          {% for item in obj.list %}
            <li>
              <span>{{item.text}}</span>
            </li>  
          {% endfor %}
      </ul>
    </div>
    <div class="card-pricing--01__button text-center">
      <a href="#" class="btn btn-60 {{" "+ obj.btnClass if obj.btnClass}}{{" active" if obj.active}}">{{obj.btnText if obj.btnText else "Get Started"}}</a>
    </div>
  </div>
{% endmacro %}
{% macro Three(obj) %}
  <div class="card-pricing--02 {{obj.modifier}}">
    <div class="card-pricing--02__head">
      <h3 class="card-pricing--02__plan-title{{" " + obj.planTextColor if obj.planTextColor}}">{{obj.plan}}</h3>
      <h2 class="card-pricing--02__price"><span>{{obj.currency}}</span>{{obj.price}}</h2>
      <span class="card-pricing--02__plan-text text-dark">Per Month</span>
    </div>
      <ul class="card-pricing--02__list list-unstyled">
          {% for item in obj.list %}
            <li>
              {% if item.disable %}
                <i class="fas fa-times-circle text-red opacity-5"></i>
                {% else %}
                <i class="fas fa-check-circle"></i>
              {% endif %}
              <span>{{item.text}}</span>
            </li>  
          {% endfor %}
      </ul>
    <div class="card-pricing--02__button">
      <a href="#" class="btn btn-60 {{" "+ obj.btnClass if obj.btnClass}}{{" active" if obj.active}}">{{obj.btnText if obj.btnText else "Get Started"}}</a>
    </div>
  </div>
{% endmacro %}
