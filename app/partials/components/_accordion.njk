{% macro one(index,obj) %}
     <div>
        <a  class="faq-accordion--01 {{"collapsed" if not obj.show}}" data-bs-toggle="collapse" href="#OneCollapseExample-{{index}}" role="button" aria-expanded="false" aria-controls="OneCollapseExample-{{index}}">
        <div class="faq-accordion__head">
            <h3 class="title heading-dark">{{obj.question}}</h3>
        </div>
        <div class="faq-accordion__body collapse {{"show" if obj.show}}" id="OneCollapseExample-{{index}}" data-bs-parent="#{{obj.parent}}">
            <p class="text-dark">{{obj.text}}.</p>
        </div>
    </a>
     </div>
{% endmacro %}
{% macro content(index,obj) %}
        <a  class="widget widget--accordion {{"collapsed" if not obj.show}}" data-bs-toggle="collapse" href="#OneCollapseExample-{{index}}" role="button" aria-expanded="false" aria-controls="OneCollapseExample-{{index}}">
            <div class="widget-icon {{obj.iconWrapperClass}}">
                <img src="{{obj.iconImage}}" alt=""/>
            </div>
            <div class="widget-texts">
                <div class="widget-texts__head">
                    <h3 class="widget-title heading-dark">{{obj.title}}</h3>
                    <i class="fas fa-chevron-down"></i>
                </div>
                <div class="widget--accordion__body collapse {{"show" if obj.show}}" id="OneCollapseExample-{{index}}" data-bs-parent="#{{obj.parent}}">
                    <p class="widget-text text-dark">{{obj.text}}.</p>
                </div>
            </div>
        </a>
{% endmacro %}
{% macro three(index,obj) %}
    <a  class="faq-accordion--02 {{"collapsed" if not obj.show}}" data-bs-toggle="collapse" href="#OneCollapseExample-{{index}}" role="button" aria-expanded="false" aria-controls="OneCollapseExample-{{index}}">
        <div class="faq-accordion__head">
            <h3 class="title heading-dark">{{obj.question}}</h3>
        </div>
        <div class="faq-accordion__body collapse {{"show" if obj.show}}" id="OneCollapseExample-{{index}}" data-bs-parent="#{{obj.parent}}">
            <p class="text-dark">{{obj.text}}.</p>
        </div>
    </a>
{% endmacro %}
